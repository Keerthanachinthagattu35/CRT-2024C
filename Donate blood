#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
   int a,w,h;
    scanf("%d%d%d",&a,&w,&h);   
    if(a=18)||(a>18)
         printf("He or she able to donate blood");
    else if(w=45)||(w>45)
         printf("He or she able to donate blood");
    else if(h=5.5)||(h>5.5)
         printf("He or she able to donate blood");
    else
         printf("He or she unable to donate blood");    
    return 0;
}
